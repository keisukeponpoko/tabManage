{
  "name": "タブ管理ツール",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "タブの管理が簡単にできます。",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "タブ管理",
    "default_popup": "popup.html"
  }
}
